  "scripts": {
    "nextversion": "bun run version.ts -i alpha",
    "version": "bun run version.ts",
    "build": "bun tsc --noEmit && bun run bun.build.ts",
    "watch": "bun tsc --noEmit && bun run bun.build.ts -w",
    "gowatch": "bun tsc --noEmit && bun run bun.build.ts -w -o ../backend/src/web/static",
    "clean": "rm -rf out",
    "dev": "bun tsc --noEmit && bun --hot run bun.build.ts -w -s ./out",
    "dev:remote": "bun tsc --noEmit && bun --hot run bun.build.ts -w -s ./out -c http://192.168.0.68:3000/",
    "gen": "rtk-query-codegen-openapi api.config.js",
    "test:prepare": "bun ./scripts/add-test-setup.js",
    "test:fix": "bun ./scripts/add-test-setup.js --apply",
    "test:ci": "bun run test:prepare && bun test",
    "lint": "bun tsc --noEmit && bun run test:prepare && bunx biome check --write"
  },
  "patchedDependencies": {},
