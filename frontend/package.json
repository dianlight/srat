  "scripts": {
    "nextversion": "bun run version.ts -i alpha",
    "version": "bun run version.ts",
    "build": "bun tsgo --noEmit && NODE_ENV=production bun run bun.build.ts",
    "watch": "bun tsgo --noEmit && NODE_ENV=development bun run bun.build.ts -w",
    "gowatch": "bun tsgo --noEmit && NODE_ENV=development API_URL=http://localhost:3000/ bun run bun.build.ts -w -o ../backend/src/web/static",
    "clean": "rm -rf out",
    "dev": "bun tsgo --noEmit && NODE_ENV=development bun --hot run bun.build.ts -w -s ./out",
    "dev:remote": "bun tsgo --noEmit && NODE_ENV=remote API_URL=http://192.168.0.68:3000/ bun --hot run bun.build.ts -w -s ./out",
    "gen": "bunx @rtk-query/codegen-openapi api.config.js",
    "test": "bun tsgo --noEmit && BUN_TEST_NO_COVERAGE_THRESHOLD=1 bun test --timeout 10000 --max-concurrency=1",
    "test:coverage": "bun test --coverage",
    "test:prepare": "bun ./scripts/add-test-setup.js",
    "test:fix": "bun ./scripts/add-test-setup.js --apply",
    "test:ci": "bun run test:prepare && bun test --only-failures --timeout 30000 --max-concurrency=1",
    "lint": "bun tsgo --noEmit && bun run test:prepare && bunx biome check --write"
  },
  "patchedDependencies": {},
  "scarfSettings": {
    "enabled": false
  },
  "trustedDependencies": [
    "@scarf/scarf",
    "@tree-sitter-grammars/tree-sitter-yaml",
    "core-js-pure",
    "tree-sitter",
    "tree-sitter-json"
