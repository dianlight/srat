{
  "conventionalCommits.scopes": [
    "api",
    "gh_actions",
    "monorepo",
    "webapp",
    "http_server",
    "samba",
    "docker",
    "sensors"
  ],
  "makefile.configureOnOpen": false,
  "remote.localPortHost": "allInterfaces",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "go.buildTags": "embedallowed_no",
  "testing.showAllMessages": true,
  "go.disableConcurrentTests": false,
  "go.testEnvVars": {
    "GOMAXPROCS": "1",
  },
  "github.copilot.chat.agent.thinkingTool": true,
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.constantValues": true,
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "file": ".github/.copilot-commit-message-instructions.md"
    },
  ],
  "editor.codeActionsOnSave": {
    "source.action.useSortedKeys.biome": "explicit",
    "source.fixAll.biome": "explicit",
  },
  "chat.agent.maxRequests": 1000,
  "geminicodeassist.agentYoloMode": true,
  "geminicodeassist.updateChannel": "Default",
  "github.copilot.chat.byok.ollamaEndpoint": "",
  "github.copilot.chat.codesearch.enabled": true,
  "githubLocalActions.actCommand": "gh act",
  "chat.tools.terminal.autoReplyToPrompts": true,
  "chat.mcp.gallery.enabled": true,
  "problems.showCurrentInStatus": true,
  "better-comments.highlightPlainText": true,
  "bookmarks.label.suggestion": "suggestWhenSelectedOrLineWhenNoSelected",
  "github.copilot.chat.alternateGptPrompt.enabled": true,
  "github.copilot.chat.completionsFetcher": "",
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.languageContext.fix.typescript.enabled": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.chat.responsesApiReasoningEffort": "default",
  "githubIssues.issueBranchTitle": "${user}/issue/${issueNumber}_${sanitizedLowercaseIssueTitle}",
  "githubIssues.issueCompletionFormatScm": "Fixes ${issueNumberLabel} - ${issueTitle}",
  "githubIssues.issueCompletions.enabled": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.ignoredError": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.installDependenciesWhenBuilding": true,
  "go.testExplorer.packageDisplayMode": "nested",
  "go.testExplorer.showDynamicSubtestsInEditor": true,
  "go.toolsManagement.autoUpdate": true,
  "javascript.preferGoToSourceDefinition": true,
  "javascript.referencesCodeLens.enabled": true
}